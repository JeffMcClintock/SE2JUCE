      <Pin name="Feedback" datatype="float" default="0.97" />
      <Pin name="Low Pass (Hz)" datatype="float" default="10000" />
    </Audio>
  </Plugin>
  
    <Plugin id="SP Reverb Zita" name="Reverb Zita" category="Effects">
