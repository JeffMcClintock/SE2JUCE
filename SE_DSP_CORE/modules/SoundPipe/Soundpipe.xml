      <Pin name="In" datatype="float" rate="audio" linearInput="true" />
      <Pin name="Out" datatype="float" rate="audio" direction="out" />
    </Audio>
  </Plugin>
  
  <Plugin id="SP Reverb SP" name="Reverb SP" category="Effects">
