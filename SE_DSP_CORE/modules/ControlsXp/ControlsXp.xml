    <GUI>
      <Pin name="Capture Data A" direction="in" datatype="blob" parameterId="0" private="true" isPolyphonic="true"/>
      <Pin name="Capture Data B" direction="in" datatype="blob" parameterId="1" private="true" isPolyphonic="true"/>
